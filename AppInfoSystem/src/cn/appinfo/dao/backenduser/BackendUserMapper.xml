<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.appinfo.dao.backenduser.BackendUserMapper">
	<select id="toLogin" resultType="backendUser">
		SELECT * FROM `backend_user` WHERE userCode=#{userCode} AND `userPassword` =#{pwd}
	</select>
</mapper>  
